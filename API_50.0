regex_lib:
    apiVersion-tag: '(?s)(\n\s*<apiVersion>___REPLACEVALUE___</apiVersion>)'

rules:
    change_api_tag_to_current_version:
        extensions:
            - xml
        regex_name: 'apiVersion-tag' 
        replace_values:
            - 50.0
        branches:
        replace_with: '<apiVersion>49.0</apiVersion>'
